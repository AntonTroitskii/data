    "def get_s(alg_predict, y):\n",
    "    s = -2*(alg_predict - y)\n",
    "    s = s.reshape(-1, 1)\n",
    "\n",
    "    return s\n",
    "\n",
    "def get_algo_predict(algo_list,ind, X):\n",
    "    end = ind\n",
    "    res = sum(algo_list[i].predict(X) for i in range(0, end))\n",
    "    res  = res.reshape(-1, 1)\n",
    "    return res"
